{
  "name": "profile_info",
  "xpath": "//main",
  "data": [
    {
      "name": "full_name",
      "xpath": "//main//header//section//span[@dir='auto' and not(ancestor::h2) and not(ancestor::a) and not(ancestor::li)][1]",
      "alternatives": [
        "//header//section//div[count(preceding-sibling::div) >= 1]//span[@dir='auto' and not(ancestor::li)]",
        "//header//h1",
        "//span[contains(@class, 'x1lliihq') and @dir='auto' and not(ancestor::li)][1]"
      ]
    },
    {
      "name": "profile_image",
      "xpath": ".//img[contains(@alt, 'profile picture')]/@src",
      "alternatives": [
        ".//header//img/@src",
        "//img[contains(@class, 'xpdipih')]/@src"
      ]
    },
    {
      "name": "post_count",
      "xpath": "//header//ul/li[1]//span",
      "alternatives": [
        "//li[contains(., 'posts')]//span",
        "//ul/li[1]//span"
      ]
    },
    {
      "name": "followers_count",
      "xpath": "//header//ul/li[2]//span",
      "alternatives": [
        "//li[contains(., 'followers')]//span",
        "//ul/li[2]//span"
      ]
    },
    {
      "name": "following_count",
      "xpath": "//header//ul/li[3]//span",
      "alternatives": [
        "//li[contains(., 'following')]//span",
        "//ul/li[3]//span"
      ]
    },
    {
      "name": "bio",
      "xpath": "//header//section//div[contains(@class, 'x1v3pt97')]//span | //header//section//div[not(@role='button')]//span",
      "alternatives": [
        "//main//header//section//span",
        "//header//section//div//span"
      ]
    },
    {
      "name": "website",
      "xpath": "//main//header//section//a[contains(@href, 'http') and not(contains(@href, 'instagram.com'))]",
      "alternatives": [
        "//main//header//section//a[@role='link' and @target='_blank']",
        "//main//header//section//button[contains(., '.com') or contains(., '.org') or contains(., '.io')]",
        "//main//header//svg[@aria-label='Link icon']/ancestor::button",
        "//main//header//a[not(contains(@href, 'instagram.com'))]"
      ]
    },
    {
      "name": "verified",
      "xpath": "//main//header//span[@title='Verified'] | //main//header//span[@aria-label='Verified']",
      "alternatives": [
        "//main//header//svg[@aria-label='Verified']",
        "//span[contains(@title, 'Verified')]",
        "//span[contains(@aria-label, 'Verified')]"
      ]
    }
  ]
}